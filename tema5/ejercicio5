def a単oBisiesto(year):
  if (year % 4 == 0):
    if (year % 100 == 0):
      if (year % 400 == 0):
        return "Es bisiesto"
      else:
        return "No es bisiesto"
    else:
      return "Es bisiesto"
  else:
    return "No es bisiesto"

print(a単oBisiesto(1900))
print(a単oBisiesto(2000))
print(a単oBisiesto(2023))
